    "source = open('pridech1.rtf')\n",
    "pride = source.read()\n",
    "def exp3(string):\n",
    "    if string != str:\n",
    "        raise TypeError(\"Input is not a string.\")\n",
    "    for element in string:\n",
    "        result = re.findall('[at]', pride)\n",
    "    return result"
